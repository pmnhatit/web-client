    <!-- Start My Account -->
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <div class="login-form">
                    <h3 class="billing-title text-center">Đăng Nhập</h3>
                    <p class="text-center mt-80 mb-40">Chào mừng bạn trở lại! Đăng nhập vào tài khoản của bạn </p>
                    <form action="homeLogin">
                        <input type="text" placeholder="Tên đăng nhập hoặc Email*" onfocus="this.placeholder=''"
                            onblur="this.placeholder = 'Tên đăng nhập hoặc Email*'" required class="common-input mt-20">
                        <input type="password" placeholder="Mật khẩu*" onfocus="this.placeholder=''"
                            onblur="this.placeholder = 'Mật khẩu*'" required class="common-input mt-20">
                        <button class="view-btn color-2 mt-20 w-100" onclick="location.href='homeLogin'"><span>Đăng Nhập</span></button>
                        <div class="mt-20 d-flex align-items-center justify-content-between">
                            <div class="d-flex align-items-center"><input type="checkbox" class="pixel-checkbox"
                                    id="login-1"><label for="login-1">Nhớ tài khoản</label></div>
                            <a href="forgetPass">Quên mật khẩu?</a>
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-md-6">
                <div class="register-form">
                    <h3 class="billing-title text-center">Đăng Ký</h3>
                    <p class="text-center mt-40 mb-30">Tạo tài khoản của riêng bạn </p>
                    <form action="#">
                        <input type="text" placeholder="Họ và tên*" onfocus="this.placeholder=''"
                            onblur="this.placeholder = 'Họ và tên*'" required class="common-input mt-20">
                        <input type="email" placeholder="Địa chỉ Email*" onfocus="this.placeholder=''"
                            onblur="this.placeholder = 'Địa chỉ Email*'" required class="common-input mt-20">
                        <input type="text" placeholder="Số điện thoại*" onfocus="this.placeholder=''"
                            onblur="this.placeholder = 'Số điện thoại*'" required class="common-input mt-20">
                        <input type="text" placeholder="Tên tài khoản*" onfocus="this.placeholder=''"
                            onblur="this.placeholder = 'Tên tài khoản*'" required class="common-input mt-20">
                        <input type="password" placeholder="Mật khẩu*" onfocus="this.placeholder=''"
                            onblur="this.placeholder = 'Mật khẩu*'" required class="common-input mt-20">
                        <button class="view-btn color-2 mt-20 w-100" ><span>Đăng Ký</span></button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- End My Account -->


